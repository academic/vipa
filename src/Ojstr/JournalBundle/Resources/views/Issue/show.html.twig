{% extends '::ojsbase.html.twig' %}
{% block body -%}
<h1>Issue</h1>
<table class="table record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Journalid</th>
            <td>{{ entity.journalId }}</td>
        </tr>
        <tr>
            <th>Volume</th>
            <td>{{ entity.volume }}</td>
        </tr>
        <tr>
            <th>Number</th>
            <td>{{ entity.number }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ entity.title }}</td>
        </tr>
        <tr>
            <th>Cover</th>
            <td>{{ entity.cover }}</td>
        </tr>
        <tr>
            <th>Special</th>
            <td>{{ entity.special }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Year</th>
            <td>{{ entity.year }}</td>
        </tr>
        <tr>
            <th>Datepublished</th>
            <td>{{ entity.datePublished|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Created</th>
            <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updated</th>
            <td>{{ entity.updated|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Deletedat</th>
            <td>{{ entity.deletedAt|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>
<hr>

                {% set back_path = 'issue' %}
                {% set edit_path = 'issue_edit' %}
                {% set delete_path = 'issue_delete' %}
                {% include '::actionbtns.html.twig' %}

{% endblock %}
