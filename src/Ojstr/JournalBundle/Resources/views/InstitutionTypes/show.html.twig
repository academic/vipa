{% extends '::ojsbase.html.twig' %}

{% block body -%}
    <h1>InstitutionTypes</h1>

    <table class="record_properties  table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('institution_types') }}">
            {% trans %}back{% endtrans %}
        </a>
    </li>
    <li>
        <a href="{{ path('institution_types_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
