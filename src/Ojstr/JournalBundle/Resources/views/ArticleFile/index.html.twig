{% extends '::ojsbase.html.twig' %}

{% block body -%}
<h1>ArticleFile list</h1>

<table class="records_list table">
    <thead>
        <tr>
            <th>Id</th>

            <th>Name</th>
            <th>Mimetype</th>
            <th>Size</th>
            <th>Articleid</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        {% for entity in entities %}
        <tr>
            <td><a href="{{ path('articlefile_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>

            <td>{{ entity.name }}</td>
            <td>{{ entity.mimeType }}</td>
            <td>{{ entity.size }}</td>
            <td>{{ entity.articleId }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('articlefile_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('articlefile_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
        {% endfor %}
    </tbody>
</table>

    {% endblock %}
