{% extends '::ojsbase.html.twig' %}

{% block body -%}
<h1>ArticleFile</h1>

<table class="record_properties table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Path</th>
            <td><small>{{ entity.path }}</small></td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Mimetype</th>
            <td>{{ entity.mimeType }}</td>
        </tr>
        <tr>
            <th>Size</th>
            <td>{{ entity.size }}</td>
        </tr>
        <tr>
            <th>Articleid</th>
            <td>{{ entity.articleId }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('articlefile') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('articlefile_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
