{% extends '::ojsbase.html.twig' %}

{% block body -%}
<h1>proxy list</h1>

<table class="table records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>proxyuserid</th>
            <th>Targetuserid</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        {% for entity in entities %}
        <tr>
            <td><a href="{{ path('admin_proxy_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.proxyUserId }}</td>
            <td>{{ entity.targetUserId }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('admin_proxy_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admin_proxy_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
        {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('admin_proxy_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
    {% endblock %}
