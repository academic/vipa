{% extends '::ojsbase.html.twig' %}

{% block body -%}
    <h1>WorkflowStep</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Journalid</th>
                <td>{{ entity.journalId }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Nextid</th>
                <td>{{ entity.nextId }}</td>
            </tr>
            <tr>
                <th>Previd</th>
                <td>{{ entity.prevId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('workflowstep') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('workflowstep_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
