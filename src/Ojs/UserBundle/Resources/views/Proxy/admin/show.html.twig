{% extends '::ojsbase.html.twig' %}

{% block body -%}
    <h1>proxy</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Proxy User id</th>
                <td>{{ entity.proxyUserId }}</td>
            </tr>
            <tr>
                <th>Client User id</th>
                <td>{{ entity.clientUserId }}</td>
            </tr>

        </tbody>
    </table>

    <ul class="record_actions">
        <a href="{{ path('admin_proxy') }}">
            {{"b"|trans}}
        </a>
        <a href="{{ path('admin_proxy_edit', { 'id': entity.id }) }}">
            Edit
        </a>
        {{ form(delete_form) }}
    </ul>
{% endblock %}
