{% extends '::ojsbase.html.twig' %}

{% block body -%}
<h1>EventLog</h1>

<table class="record_properties  table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Eventinfo</th>
            <td>{{ entity.eventInfo }}</td>
        </tr>
        <tr>
            <th>Eventdate</th>
            <td>{{ entity.eventDate|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Ip</th>
            <td>{{ entity.ip }}</td>
        </tr>
        <tr>
            <th>Userid</th>
            <td>{{ entity.userId }}</td>
        </tr>
    </tbody>
</table>


<a class="btn btn-default" href="{{ path('eventlog') }}">
    {{"b"|trans}}
</a>
</ul>
{% endblock %}
