        {'link': path('ujr'),'title': 'user.journalrole'|trans},
        {'title': 'user.journalrole.edit'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
<div class="large-12 columns">
    <div class="panel">
        <h1>{{ "user.journalrole.edit"|trans }}</h1>
