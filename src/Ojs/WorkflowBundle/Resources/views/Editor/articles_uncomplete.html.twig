{% extends '::ojsbase.html.twig' %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'},
    {'title': 'articles.uncomplete'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <div class="panel">
        <div class="panel-heading">
            <h1>Uncomplete Articles
                <small>These articles not submitted yet</small>
            </h1>
        </div>

        <div class="panel-body">
            <div class="list-group-item">
                {{ grid(grid) }}
            </div>
        </div>
    </div>
{% endblock %}
