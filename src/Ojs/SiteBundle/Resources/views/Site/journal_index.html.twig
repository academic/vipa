{% extends "::ojsbase.html.twig" %}
{% set hideleft = 1%}
{%block breadcrumb %}{% endblock %}
{% block body %}

    <div class="container">
        <div class="page-header">
            <h1>Cumhuriyet Science Journal</h1>
            <h6>Cumhuriyet University Science Journal is an official publication of Cumhuriyet University Science Faculty. </h6>
        </div>

        <div class="col-lg-9 col-md-9 col-sm-12">
            <div class="row">
                <div class="col-md-4">
                    <img src="holder.js/189x296/#f9A497:#ffffff/text:cover" class="img-rounded float-left" alt="" />
                </div>

                <div class="col-md-8">
                    <p class="text-default">Cumhuriyet University Science Journal is an official publication of Cumhuriyet University Science Faculty. The publications related to the natural sciences and engineering are published nationally and internationally. This journal have been published two times a year.
                    </p>
                </div>
                <hr>
                <div class="col-md-8">
                    <a href="/journal/{{ entity.id }}/articles" class="btn btn-alt btn-xs btn-sky"><i class="fa fa-eye"></i> Son Sayıyı Oku</a>

                    <a href="/journal/{{ entity.id }}/archive" class="btn btn-alt btn-xs btn-sunny"><i class="fa fa-archive"></i> Arşiv</a>
                    <p class="text-default">
                        <span class="label label-info"><strong>ISSN</strong>: 2148-7510</span>
                        <span class="label label-info"><strong>e-ISSN</strong>: 2148-7758</span>
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <h4 class="page-header"><span class="panel-title">Editoryal Bilgiler</span></h4>
                    <p>
                        <strong>Editor</strong>
                        <br>
                        Prof. Dr. Rauf AMİROV
                        <br>
                        <strong>Assistant Editors</strong><br>
                        Assoc. Prof. Dr. Nilüfer TOPSAKAL<br>
                        Assoc. Prof. Dr. Şifa TÜRKOĞLU<br>
                        Assist. Prof. Dr. Nüket KARTAL TEMEL<br>
                        Assist. Prof. Dr. Serkan AKKOYUN<br>
                    </p>
<table class="record_properties  table">

                <tbody>
                    <tr>
                        <th width="300px">Id</th>
                        <td>{{ entity.id }}</td>
                    </tr>
                    <tr>
                        <th>Title</th>
                        <td>{{ entity.title }}</td>
                    </tr>
                    <tr>
                        <th>Titleabbr</th>
                        <td>{{ entity.titleAbbr }}</td>
                    </tr>
                    <tr>
                        <th>Titletransliterated</th>
                        <td>{{ entity.titleTransliterated }}</td>
                    </tr>
                    <tr>
                        <th>Journal Languages</th>
                        <td>{% for language in entity.languages %}{{language.name}}{%endfor%}</td>
                    </tr>

                    <tr>
                        <th>Subtitle</th>
                        <td>{{ entity.subtitle }}</td>
                    </tr>
                    <tr>
                        <th>Issn</th>
                        <td>{{ entity.issn }}</td>
                    </tr>
                    <tr>
                        <th>Eissn</th>
                        <td>{{ entity.eissn }}</td>
                    </tr>
                    <tr>
                        <th>Firstpublishdate</th>
                        <td>{{ entity.firstPublishDate|date('Y-m-d H:i:s') }}</td>
                    </tr>
                    <tr>
                        <th>Period</th>
                        <td>{{ entity.period }}</td>
                    </tr>
                    <tr>
                        <th>Url</th>
                        <td>{{ entity.url }}</td>
                    </tr>
                    <tr>
                        <th>Country</th>
                        <td>{{ entity.country }}</td>
                    </tr>
                    <tr>
                        <th>published</th>
                        <td>{{ entity.published }}</td>
                    </tr>
                    <tr>
                        <th>Status</th>
                        <td>{{ entity.status }}</td>
                    </tr>
                    <tr>
                        <th>Image</th>
                        <td>{{ entity.image }}</td>
                    </tr>
                    <tr>
                        <th>Scope</th>
                        <td>{{ entity.scope }}</td>
                    </tr>
                    <tr>
                        <th>Mission</th>
                        <td>{{ entity.mission }}</td>
                    </tr>
                    <tr>
                        <th>Themeid</th>
                        <td>{{ entity.themeId }}</td>
                    </tr>
                    <tr>
                        <th>Users</th>
                        <td><a href="{{ path('ujr_show_users_ofjournal',{ 'journal_id': entity.id }) }}">Show Users</a>
                        </td>
                    </tr>
                </tbody>
            </table>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">



            <div class="panel panel-default">
                <div class="panel-heading"><a href="#"><span class="panel-title"><i class="fa fa-plus"></i> Blok ekle</span></a></div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading"><span class="panel-title">Okuyucular için</span></div>
                <ul class="list-group">
                    <li class="list-group-item">
                        <a href="/articles"><i class="fa fa-eye"></i> Son Sayıyı Oku</a>
                    </li>
                    <li class="list-group-item">
                        <a href="/archive"><i class="fa fa-archive"></i> Arşiv</a>
                    </li>
                    <li class="list-group-item"><a href="#"><i class="fa fa-plus"></i> <strong>Link ekle</strong></a></li>
                </ul>
            </div>

            <div class="panel panel-warning">
                <div class="panel-heading"><span class="panel-title">Yazarlar ve Editörler için</span></div>
                <ul class="list-group">
