{% extends '::ojsbase.html.twig' %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'},
    {'title': '<strong>Page Management</strong>' },
    { 'title': 'Edit Pages' }
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>Edit Page</h1>

    {{ form(edit_form) }}

    <div class="record_actions">
        {% set back_path = 'admin_page' %}
        {% set delete_path = 'admin_page_delete' %}
        {% include '::actionbtns.html.twig' %}
    </div>
{% endblock %}
