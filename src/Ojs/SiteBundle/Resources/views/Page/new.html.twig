{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.page_new'|trans }} {{ parent() }}{% endblock %}
{% block breadcrumb %}
    {% set list = [
        {'link': path('dashboard'), 'title': 'dashboard'},
        {'title': 'pages.management'|trans },
        { 'title': 'pages.new'|trans }
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>New Page</h1>

    {{ form(form) }}
    {% set back_path = 'admin_page' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
