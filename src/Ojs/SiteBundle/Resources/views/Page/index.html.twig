{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.pages'|trans }} {{ parent() }}{% endblock %}
{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'},
    {'title': 'pages.management'|trans },
    { 'title': 'pages.all'|trans }
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>All Pages</h1>
    {% include '::flashbag.html.twig' %}
    {{ grid(grid) }}
    {% set create_path = 'admin_page_new' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
