{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.publisher_application'|trans }} {{ parent() }}{% endblock %}
{% set hideleft = 1 %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('ojs_public_index'), 'title': 'ojs.title'|trans},
    {'title': 'application.publisher.apply'|trans},
    {'title': 'success'}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body %}
    <div class="container">
        <!-- Page Content -->
        <h1 class="page-header">{{ "success"|trans }}</h1>

        <div class="jumbotron">
            {{ "Your application saved successfully."|trans }}
        </div>
    </div>
{% endblock %}
