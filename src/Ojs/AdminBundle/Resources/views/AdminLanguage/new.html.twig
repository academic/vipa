{% extends '::ojsbase.html.twig' %}
{% set hideleft = true %}
{% block title %}{{ 'title.language_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
        {'link': path('ojs_admin_dashboard'), 'title': 'dashboard'|trans},
        {'link': path('ojs_admin_language_index'),'title': 'title.languages'|trans},
        {'title': 'title.language_show'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ "lang.new"|trans }}</h1>

    {{ form(form) }}

    {% set back_path = 'ojs_admin_language_index' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
