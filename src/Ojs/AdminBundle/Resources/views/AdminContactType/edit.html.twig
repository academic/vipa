{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.contact_type_edit'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
        {'link': path('ojs_admin_dashboard'), 'title': 'dashboard'|trans},
        { 'link': path('ojs_admin_contact_type_index'),'title': 'title.contact_types'|trans},
        {'title': 'title.contact_type_edit'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ "contact.type"|trans }}</h1>
    {% include '::flashbag.html.twig' %}
    {{ form(edit_form) }}

    {% set back_path = 'ojs_admin_contact_type_index' %}
    {% if is_granted('DELETE', entity) %}
        {% set delete_path = 'ojs_admin_contact_type_delete' %}
    {% endif %}
    {% include '::actionbuttons.html.twig' %}

    {{ getLogs(entity) }}
{% endblock %}
