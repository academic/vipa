{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.mail_logs'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'|trans},
    {'link': path('ojs_admin_mail_log_index'),'title': 'maillog.singular'|trans},
    {'title': 'list'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ "maillog.list" }}</h1>
    {% include '::flashbag.html.twig' %}
    {{ grid(grid) }}
    {% if is_granted('CREATE', getEntity('OjsUserBundle:MailLog')) %}
        {% set create_path = 'ojs_admin_mail_log_new' %}
    {% endif %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
