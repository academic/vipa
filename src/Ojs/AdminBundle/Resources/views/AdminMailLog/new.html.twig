{% extends '::ojsbase.html.twig' %}
{% set hideleft = true %}
{% block title %}{{ 'title.mail_log_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'|trans},
    {'link': path('ojs_admin_mail_log_index'),'title': 'maillog.singular'|trans},
    {'title': 'new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ "maillog.creation" }}</h1>

    {{ form(form) }}

    {% set back_path = 'ojs_admin_mail_log_index' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
