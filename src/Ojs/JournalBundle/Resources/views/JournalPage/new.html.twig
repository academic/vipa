{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.page_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
        {'link': path('ojs_admin_dashboard'), 'title': 'dashboard'|trans},
        {'link': path('ojs_journal_page_index', {'journalId': selectedJournal().id}),'title': 'title.pages'|trans},
        {'title': 'title.page_new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ 'title.page_new'|trans }}</h1>
    {{ form(form) }}
    {{ actions({'back': {'href': path('ojs_journal_page_index', {'journalId': selectedJournal().id})}}) }}
{% endblock %}
