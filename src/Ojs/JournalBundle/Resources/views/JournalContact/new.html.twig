{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.journal_contact_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'|trans},
    {'link': path('ojs_journal_journal_contact_index', {'journalId': selectedJournal().id}),'title': 'journal.contact'|trans},
    {'title': 'title.journal_contact_new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ 'title.journal_contact_new'|trans }}</h1>

    {{ form(form) }}
    {{
    actions({
        'back': {'href': path('ojs_journal_journal_contact_index', {'journalId': selectedJournal().id})},
    })
    }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('/bundles/ojslocation/js/location.js') }}"></script>
{% endblock %}
