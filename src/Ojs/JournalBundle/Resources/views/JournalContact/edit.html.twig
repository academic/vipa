{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.journal_contact_edit'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('ojs_journal_journal_contact_index', {'journalId': selectedJournal().id}), 'title': 'dashboard'|trans},
    {'link': path(index_path),'title': 'journal.contact'|trans},
    {'title': 'title.journal_contact_edit'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ 'title.journal_contact_edit'|trans }}</h1>
    {% include '::flashbag.html.twig' %}
    {{ form(edit_form) }}
    {{
    actions({
        'back': {'href': path('ojs_journal_journal_contact_index', {'journalId': selectedJournal().id})},
        'delete': {'href': path('ojs_journal_journal_contact_delete', {'id': entity.id, 'journalId': selectedJournal().id}), 'attributes': {'data-token': token}},
    })
    }}

    {{ getLogs(entity) }}
{% endblock %}
