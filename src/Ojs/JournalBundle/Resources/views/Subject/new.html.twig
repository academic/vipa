{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.subject_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'|trans},
    {'link': path('subject'),'title': 'subject'|trans},
    {'title': 'new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ "subject.new"|trans }}</h1>

    {{ form(form) }}

    {% set back_path = 'subject' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
