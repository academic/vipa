{% extends '::ojsbase.html.twig' %}

{% block body -%}
    <h1>{{"journal.design"|trans}}</h1>
    {% include '::flashbag.html.twig' %}
    <table class="record_properties">
        <tbody>
            <tr>
                <th>{{ 'id'|trans }}</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>{{"journal.id"|trans}}</th>
                <td>{{ entity.journalId }}</td>
            </tr>
            <tr>
                <th>{{"theme.id"|trans}}</th>
                <td>{{ entity.themeId }}</td>
            </tr>
        </tbody>
    </table>
    {% set back_path = 'admin_journaldesign' %}
    {% set edit_path = 'admin_journaldesign_edit' %}
    {% set delete_path = 'admin_journaldesign_delete' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
