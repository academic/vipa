{% extends '::ojsbase.html.twig' %}

{% block body -%}
    <h1>{{"journaltheme"|trans}}</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>{{"journal.id"|trans}}</th>
                <td>{{ entity.journalId }}</td>
            </tr>
            <tr>
                <th>{{"theme.id"|trans}}</th>
                <td>{{ entity.themeId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_journaltheme') }}">
            {{"b"|trans}}
        </a>
    </li>
    <li>
        <a href="{{ path('admin_journaltheme_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
