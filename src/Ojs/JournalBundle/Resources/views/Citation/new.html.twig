{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.citation_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'|trans},
    { 'link': path('citation'),'title': 'citation'|trans},
    {'title': 'new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ "citation.create"|trans }}</h1>

    {{ form(form) }}
    {% set back_path = 'citation' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
