{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.contact_edit'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'|trans},
    {'link': path('contact'),'title': 'contact.manager'|trans},
    {'title': 'contact.edit'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    {% include '::flashbag.html.twig' %}
    {{ form(edit_form) }}

    {% set back_path = 'contact' %}
    {% if is_granted('DELETE', entity) %}
        {% set delete_path = 'contact_delete' %}
    {% endif %}
    {% include '::actionbuttons.html.twig' %}

    {{ getLogs(entity) }}
{% endblock %}
