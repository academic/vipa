{% extends '::ojsbase.html.twig' %}

{% block body -%}
    <h1>{{ 'journalindex.list' }}</h1>

    {% include '::flashbag.html.twig' %}
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ entity.logo }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
        </tbody>
    </table>
    {% set back_path = 'admin_journalindex' %}
    {% set edit_path = 'admin_journalindex_edit' %}
    {% set delete_path = 'admin_journalindex_delete' %}
    {% include '::actionbtns.html.twig' %}
{% endblock %}
