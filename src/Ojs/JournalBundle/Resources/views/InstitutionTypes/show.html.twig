{% extends '::ojsbase.html.twig' %}

{% block body -%}
    <h1>{{"institutiontype.list"|trans}}</h1>
    {% include '::flashbag.html.twig' %}
    <table class="record_properties  table">
        <tbody>
            <tr>
                <th>{{ 'id'|trans }}</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>{{"name"|trans}}</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>{{"description"|trans}}</th>
                <td>{{ entity.description }}</td>
            </tr>
        </tbody>
    </table>
    {% set back_path = 'institution_types' %}
    {% set edit_path = 'institution_types_edit' %}
    {% set delete_path = 'institution_types_delete' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
