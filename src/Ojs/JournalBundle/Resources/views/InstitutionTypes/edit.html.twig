{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.institution_type_edit'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
        {'link': path('dashboard'), 'title': 'dashboard'|trans},
        {'link': path('institution_types'),'title': 'institutiontype'|trans},
        {'title': 'edit'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{"institutiontype.edit"|trans}}</h1>
    {% include '::flashbag.html.twig' %}
    {{ form(edit_form) }}
    {% set back_path = 'institution_types' %}
    {% if is_granted('DELETE', edit_form.vars.data) %}
        {% set delete_path = 'institution_types_delete' %}
    {% endif %}
    {% include '::actionbuttons.html.twig' %}

    {{ getLogs(entity) }}
{% endblock %}
