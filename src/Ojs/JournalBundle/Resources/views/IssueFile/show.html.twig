{% extends '::ojsbase.html.twig' %}

{% block body -%}
    <h1>IssueFile</h1>

    <table class="record_properties  table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Fileid</th>
                <td>{{ entity.fileId }}</td>
            </tr>
            <tr>
                <th>Issueid</th>
                <td>{{ entity.issueId }}</td>
            </tr>
            <tr>
                <th>Version</th>
                <td>{{ entity.version }}</td>
            </tr>
            <tr>
                <th>Keywords</th>
                <td>{{ entity.keywords }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
            <tr>
                <th>Langcode</th>
                <td>{{ entity.langCode }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ entity.updated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Deletedat</th>
                <td>{{ entity.deletedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Createdby</th>
                <td>{{ entity.createdBy }}</td>
            </tr>
            <tr>
                <th>Updatedby</th>
                <td>{{ entity.updatedBy }}</td>
            </tr>
        </tbody>
    </table>

    {{
    actions({
        'back': {'href': path('ojs_journal_issue_file_index', {'issueId': entity.issueId, 'journalId': selectedJournal().id})},
        'edit': {'href': path('ojs_journal_issue_file_edit', {'id': entity.id, 'issueId': entity.issueId, 'journalId': selectedJournal().id})},
        'delete': {'href': path('ojs_journal_issue_file_delete', {'id': entity.id, 'issueId': entity.issueId, 'journalId': selectedJournal().id}), 'attributes': {'data-token': token}},
    })
    }}
</ul>
{% endblock %}
