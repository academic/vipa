{% extends '::base.html.twig' %}

{% block body -%}
    <h1>IssueFile list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>Fileid</th>
                <th>Issueid</th>
                <th>Version</th>
                <th>Keywords</th>
                <th>Description</th>
                <th>Title</th>
                <th>Langcode</th>
                <th>Updated</th>
                <th>Deletedat</th>
                <th>Createdby</th>
                <th>Updatedby</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('ojs_journal_issue_file_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.type }}</td>
                <td>{{ entity.fileId }}</td>
                <td>{{ entity.issueId }}</td>
                <td>{{ entity.version }}</td>
                <td>{{ entity.keywords }}</td>
                <td>{{ entity.description }}</td>
                <td>{{ entity.title }}</td>
                <td>{{ entity.langCode }}</td>
                <td>{% if entity.updated %}{{ entity.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.deletedAt %}{{ entity.deletedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.createdBy }}</td>
                <td>{{ entity.updatedBy }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('ojs_journal_issue_file_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('ojs_journal_issue_file_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('ojs_journal_issue_file_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
