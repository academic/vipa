{% extends '::ojsbase.html.twig' %}

{% block body -%}
    {% include 'OjsJournalBundle:Journal:journal_settings_nav.html.twig' with {'active':2} only %}
    <h1>{{ 'journalsindex.create'|trans }}</h1>

    {{ form(form) }}

    {{ form_start(form) }}
    {{ form_errors(form) }}
    {{ form_widget(form) }}

    <button type="submit" id="ojs_userbundle_user_submit" class="btn btn-primary">
        {{ "create"|trans }}
    </button>
    <a class="btn btn-default" href="{{ path('manager_journals_indexes',{journal: entity.journalId}) }}">
        {{ 'b'|trans }}
    </a> 
    {{ form_end(form) }}
{% endblock %}
