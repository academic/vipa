{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.articles'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('dashboard'), 'title': 'dashboard'|trans},
    {'title': 'article.journal'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    {% include '::flashbag.html.twig' %}
    {{ "articles_in_journal"|trans }} <strong>{{ journal.title }}</strong>
    {% set back_path = 'article' %}
    {% set create_path = 'article_new' %}
    {% include '::actionbuttons.html.twig' %}
    {% include "OjsJournalBundle:Article:index_list_block.html.twig" %}
{% endblock %}
