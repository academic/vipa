{% extends '::ojsbase.html.twig' %}


{% block breadcrumb %}
    {% set list = [
                {'link': path('dashboard'), 'title': 'Dashboard'},
                { 'title': 'Reports'}
] %}
    {{breadcrumb(list)}}
{% endblock %}


{% block body -%} 
    <div class="col-md-12">
        <h2>Reports</h2>

        <a class="btn btn-lg btn-default"><i class="fa fa-users"></i> {{"reports.user"|trans }}</a>
        <a class="btn btn-lg btn-default"><i class="fa fa-file"></i> {{"reports.article"|trans }}</a>
        <a class="btn btn-lg btn-default"><i class="fa fa-line-chart"></i> {{"reports.analytics"|trans }}</a>

    </div>
{% endblock %}
