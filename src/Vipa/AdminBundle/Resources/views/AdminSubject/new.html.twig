{% extends '::vipabase.html.twig' %}
{% set hideleft = true %}
{% block title %}{{ 'title.subject_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
        {'link': path('vipa_admin_dashboard'), 'title': 'dashboard'|trans},
        {'link': path('vipa_admin_subject_index'),'title': 'title.subjects'|trans},
        {'title': 'title.subject_new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ "subject.new"|trans }}</h1>

    {{ form(form) }}

    {% set back_path = 'vipa_admin_subject_index' %}
    {% include '::actionbuttons.html.twig' %}
{% endblock %}
